    <script src="https://cdn.jsdelivr.net/npm/flatpickr"></script>
    <script>
        // Loading indicator functions
        function showLoading() {
            const loadingContainer = document.getElementById('loadingContainer');
            if (loadingContainer) {
                loadingContainer.classList.add('active');
            }
        }

        function hideLoading() {
            const loadingContainer = document.getElementById('loadingContainer');
            if (loadingContainer) {
                loadingContainer.classList.remove('active');
            }
        }

        // Fallback error message function if not defined elsewhere
        if (typeof showErrorMessage !== 'function') {
            window.showErrorMessage = function(message) {
                console.error('Error:', message);
                
                // Use the notification function if available
                if (typeof showNotification === 'function') {
                    showNotification(message, 'error', 5000);
                } else {
                    alert(message);
                }
            };
        }
        
        // Completely replace the old showNotification function with our modern version
        window.showNotification = function(message, type = 'success', duration = 3000) {
            console.log(`[MODERN NOTIFICATION SYSTEM] ${message} (${type})`);
            
            // Track notification counts for debugging
            window.notificationCount = window.notificationCount || 0;
            window.notificationCount++;
            
            // Keep track of active notifications
            window.activeNotifications = window.activeNotifications || [];
            
            // Force cleanup of any existing notifications (both old and new)
            const oldNotifications = document.querySelectorAll('.notification');
            oldNotifications.forEach(notification => {
                try {
                    if (notification.classList.contains('show')) {
                        notification.classList.remove('show');
                    }
                    setTimeout(() => {
                        if (document.body.contains(notification)) {
                            document.body.removeChild(notification);
                        }
                    }, 300);
                } catch (e) {
                    console.error('Error cleaning up old notification:', e);
                }
            });
            
            // Clean up modern notifications
            const existingNotifications = document.querySelectorAll('.modern-notification-container');
            existingNotifications.forEach(notification => {
                try {
                    notification.classList.add('hide');
                    setTimeout(() => {
                        if (document.body.contains(notification)) {
                            notification.remove();
                        }
                    }, 400); // Match transition duration
                } catch (e) {
                    console.error('Error cleaning up modern notification:', e);
                }
            });
            
            // Create the notification container with unique ID for tracking
            const notificationId = `notification-${Date.now()}-${window.notificationCount}`;
            const container = document.createElement('div');
            container.id = notificationId;
            container.className = 'modern-notification-container';
            
            // Create appropriate icon based on notification type
            let icon = '';
            switch(type) {
                case 'success':
                    icon = '<svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path></svg>';
                    break;
                case 'error':
                    icon = '<svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"></path></svg>';
                    break;
                case 'warning':
                    icon = '<svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 9v2m0 4h.01m-6.938 4h13.856c1.54 0 2.502-1.667 1.732-3L13.732 4c-.77-1.333-2.694-1.333-3.464 0L3.34 16c-.77 1.333.192 3 1.732 3z"></path></svg>';
                    break;
                case 'info':
                default:
                    icon = '<svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 16h-1v-4h-1m1-4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path></svg>';
                    break;
            }
            
            // Create notification content with our new class names
            container.innerHTML = `
                <div class="modern-notification-alert ${type}">
                    ${icon}
                    <p>${message}</p>
                </div>
            `;
            
            // Add notification to DOM
            document.body.appendChild(container);
            
            // Track this notification
            window.activeNotifications.push({
                id: notificationId,
                element: container,
                addedAt: Date.now(),
                type: type,
                message: message
            });
            
            // Log current state for debugging
            console.log(`Active notifications: ${window.activeNotifications.length}`);
            
            // Ensure it's properly inserted before showing with a slightly longer delay
            setTimeout(() => {
                if (document.body.contains(container)) {
                    container.classList.add('show');
                }
            }, 100);
            
            // Remove after duration
            setTimeout(() => {
                if (document.body.contains(container)) {
                    container.classList.add('hide');
                    
                    // Update tracking
                    window.activeNotifications = window.activeNotifications.filter(n => n.id !== notificationId);
                    console.log(`Notification removed. Active count: ${window.activeNotifications.length}`);
                    
                    setTimeout(() => {
                        if (document.body.contains(container)) {
                            container.remove();
                        }
                    }, 400); // Match transition duration
                }
            }, duration);
            
            // Return the notification for potential programmatic control
            return container;
        };
        
        // Create a test function to demonstrate all notification types
        function testNotifications() {
            console.log("TESTING MODERN NOTIFICATION SYSTEM");
            console.log("==================================");
            
            // Reset notification counts
            window.notificationCount = 0;
            window.activeNotifications = [];
            
            // Display success notification immediately
            showNotification("SUCCESS: This is a modern notification system", "success", 3000);
            
            // Display info notification after 4 seconds
            setTimeout(() => {
                showNotification("INFO: Modern notification system - test 2", "info", 3000);
            }, 4000);
            
            // Display warning notification after 8 seconds
            setTimeout(() => {
                showNotification("WARNING: Modern notification system - test 3", "warning", 3000);
            }, 8000);
            
            // Display error notification after 12 seconds
            setTimeout(() => {
                showNotification("ERROR: Modern notification system - test 4", "error", 3000);
            }, 12000);
            
            // Final notification after all others to confirm
            setTimeout(() => {
                showNotification("All notification tests completed", "success", 3000);
            }, 16000);
        }
        
        // Document ready handler
        document.addEventListener('DOMContentLoaded', function() {
            hideLoading();
            
            // Check if we just changed branches
            if (sessionStorage.getItem('branchJustChanged') === 'true') {
                console.log('Page loaded after branch change');
                const previousBranchId = sessionStorage.getItem('previousBranchId');
                
                // Clear the flag
                sessionStorage.removeItem('branchJustChanged');
                sessionStorage.removeItem('previousBranchId');
                
                // Show a brief notification
                if (typeof showNotification === 'function') {
                    try {
                        showNotification('Branch changed successfully', 'success', 2000);
                    } catch (e) {
                        // If showNotification isn't available or fails, use a simpler approach
                        console.log('Branch changed successfully:', previousBranchId);
                    }
                }
            }
            
            // Branch selector functionality
            const branchSelect = document.getElementById('branchSelect');
            if (branchSelect) {
                // Load branches into the dropdown
                async function loadBranchOptions() {
                    try {
                        if (!window.hotelBranchManager) return;
                        
                        const branches = await window.hotelBranchManager.getAllBranches();
                        
                        // Clear existing options
                        while (branchSelect.options.length > 1) {
                            branchSelect.remove(1);
                        }
                        
                        // Add branch options
                        branches.forEach(branch => {
                            const option = document.createElement('option');
                            option.value = branch.id;
                            option.textContent = branch.name;
                            branchSelect.appendChild(option);
                        });
                        
                        // Set currently selected branch if any
                        const currentBranch = window.hotelBranchManager.getCurrentBranch();
                        if (currentBranch) {
                            branchSelect.value = currentBranch;
                        }
                    } catch (error) {
                        console.error('Error loading branches:', error);
                    }
                }
                
                // Handle branch selection change
                branchSelect.addEventListener('change', async function() {
                    const selectedBranchId = this.value;
                    if (!selectedBranchId) return;
                    
                    // Reset the flag that stops branch checks when a branch is selected
                    window.stopBranchChecks = false;
                    
                    try {
                        // Immediately update UI to show loading state before any async operations
                        // This prevents the default branch name from briefly showing
                        const branchNameEl = document.getElementById('branchName');
                        const branchAddressEl = document.getElementById('branchAddress');
                        
                        if (branchNameEl) {
                            // Preserve SVG icon
                            const svgIcon = branchNameEl.querySelector('svg');
                            if (svgIcon) {
                                branchNameEl.innerHTML = '';
                                branchNameEl.appendChild(svgIcon);
                                
                                // Create loading text with animation
                                const loadingText = document.createElement('span');
                                loadingText.className = 'loading-text';
                                loadingText.textContent = ' Loading';
                                
                                // Create dots with animation
                                const loadingDots = document.createElement('span');
                                loadingDots.className = 'loading-dots';
                                
                                branchNameEl.appendChild(loadingText);
                                branchNameEl.appendChild(loadingDots);
                            } else {
                                branchNameEl.innerHTML = '<span class="loading-text">Loading</span><span class="loading-dots"></span>';
                            }
                        }
                        
                        if (branchAddressEl) {
                            // Preserve SVG icon
                            const svgIcon = branchAddressEl.querySelector('svg');
                            if (svgIcon) {
                                branchAddressEl.innerHTML = '';
                                branchAddressEl.appendChild(svgIcon);
                                branchAddressEl.appendChild(document.createTextNode(' Please wait, changing branch'));
                            } else {
                                branchAddressEl.textContent = 'Please wait, changing branch';
                            }
                        }
                        
                        // Show loading indicator
                        showLoading();
                        console.log('Loading branch data for:', selectedBranchId);
                        
                        // Load branch data
                        await window.hotelBranchManager.loadBranchData(selectedBranchId);
                        
                        // Set a flag in sessionStorage to indicate we just changed branches
                        // This helps with debugging and can be used to show messages on reload
                        sessionStorage.setItem('branchJustChanged', 'true');
                        sessionStorage.setItem('previousBranchId', selectedBranchId);
                        
                        console.log('Branch data loaded successfully, reloading page');
                        
                        // Refresh the page to update all data
                        window.location.reload();
                    } catch (error) {
                        console.error('Error selecting branch:', error);
                        hideLoading();
                        
                        // Reset the branch selector to the current branch
                        const currentBranch = window.hotelBranchManager.getCurrentBranch();
                        if (currentBranch) {
                            branchSelect.value = currentBranch;
                        }
                        
                        alert('Failed to select branch: ' + (error.message || 'Unknown error. Please try again.'));
                    }
                });
                
                // Load branch options
                loadBranchOptions();
            }
            
            // Time selection functionality - use event delegation for better performance
            const timeSelectionButtons = document.getElementById('timeSelectionButtons');
            const dateTimeContainer = document.getElementById('dateTimeContainer');
            const timePreferenceInput = document.getElementById('timePreference');
            
            if (timeSelectionButtons) {
                timeSelectionButtons.addEventListener('click', function(e) {
                    const button = e.target.closest('.time-btn');
                    if (!button) return;
                    
                    // Remove active class from all buttons
                    const buttons = this.querySelectorAll('.time-btn');
                    buttons.forEach(btn => btn.classList.remove('active'));
                    
                    // Add active class to clicked button
                    button.classList.add('active');
                    
                    // Update hidden input value
                    const timeValue = button.dataset.time;
                    if (timePreferenceInput) {
                        timePreferenceInput.value = timeValue;
                    }
                    
                    // Show/hide date time container
                    if (dateTimeContainer) {
                        dateTimeContainer.style.display = (timeValue === 'before' || timeValue === 'after') ? 'block' : 'none';
                    }
                });
            }
            
            // Form submission with optimized handling
            const form = document.getElementById('issueForm');
            if (form) {
                form.addEventListener('submit', function(e) {
                    e.preventDefault();
                    showLoading();
                    
                    const submitBtn = document.getElementById('submitButton');
                    if (submitBtn) {
                        const originalText = submitBtn.innerHTML;
                        submitBtn.disabled = true;
                        submitBtn.innerHTML = '<span class="spinner"></span> Submitting...';
                        
                        // Simulate form submission (replace with actual API call)
                        setTimeout(() => {
                            hideLoading();
                            submitBtn.innerHTML = '<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><polyline points="20 6 9 17 4 12"></polyline></svg> Success!';
                            submitBtn.classList.add('success');
                            
                            // Reset form after delay
                            setTimeout(() => {
                                form.reset();
                                submitBtn.disabled = false;
                                submitBtn.classList.remove('success');
                                submitBtn.innerHTML = originalText;
                                
                                // Update dashboard stats
                                const newIssuesCount = document.getElementById('newIssuesCount');
                                if (newIssuesCount) {
                                    newIssuesCount.textContent = parseInt(newIssuesCount.textContent || 0) + 1;
                                }
                            }, 1500);
                        }, 1000);
                    }
                });
            }
            
            // Initialize dashboard stats
            initializeDashboardStats();
            
            // Note: branch info will be updated by window.onload only to prevent recursion
        });
        
        // Update branch information in the UI
        function updateBranchInfoUI() {
            // Global flag to completely stop checks after a reasonable time
            if (window.stopBranchChecks === true) {
                console.log('Branch checks stopped. Please select a branch to continue.');
                return;
            }
            
            // Prevent excessive update attempts by tracking last attempt time
            const now = Date.now();
            if (window.lastBranchUpdateAttempt && (now - window.lastBranchUpdateAttempt) < 500) {
                // Skip this update if we've attempted too recently
                return;
            }
            window.lastBranchUpdateAttempt = now;
            
            // Track call count for diagnostic purposes
            window.branchUpdateCallCount = (window.branchUpdateCallCount || 0) + 1;
            
            if (!window.hotelBranchManager) {
                console.warn('hotelBranchManager not available');
                // Don't schedule another check - we'll rely on the window.onload check
                return;
            }
            
            try {
                const branchData = window.hotelBranchManager.getBranchData();
                if (!branchData) {
                    console.warn('No branch data available');
                    
                    // Check if a branch is selected but data hasn't loaded yet
                    const branchId = window.hotelBranchManager.getCurrentBranch();
                    if (branchId) {
                        console.log('Branch selected but data not loaded yet, attempting to load:', branchId);
                        
                        // Update UI to show loading state
                        const branchNameEl = document.getElementById('branchName');
                        if (branchNameEl) {
                            // Preserve SVG icon
                            const svgIcon = branchNameEl.querySelector('svg');
                            if (svgIcon) {
                                branchNameEl.innerHTML = '';
                                branchNameEl.appendChild(svgIcon);
                                
                                // Create loading text with animation
                                const loadingText = document.createElement('span');
                                loadingText.className = 'loading-text';
                                loadingText.textContent = ' Loading branch data';
                                
                                // Create dots with animation
                                const loadingDots = document.createElement('span');
                                loadingDots.className = 'loading-dots';
                                
                                branchNameEl.appendChild(loadingText);
                                branchNameEl.appendChild(loadingDots);
                            }
                        }
                        
                        // Try to reload the branch data, but prevent recursive calling
                        if (!window.branchDataLoadInProgress) {
                            window.branchDataLoadInProgress = true;
                            window.hotelBranchManager.loadBranchData(branchId)
                                .then(() => {
                                    console.log('Branch data loaded successfully on retry');
                                    window.branchDataLoadInProgress = false;
                                    // Try updating UI again after a short delay
                                    setTimeout(updateBranchInfoUI, 300);
                                })
                                .catch(err => {
                                    console.error('Error loading branch data on retry:', err);
                                    window.branchDataLoadInProgress = false;
                                });
                        }
                    } else {
                        // No branch selected - log once and don't schedule more checks
                        if (!window.noBranchMessageShown) {
                            window.noBranchMessageShown = true;
                            console.log('No branch selected, waiting for user selection');
                        }
                        return; // Don't schedule more updates - we'll check one more time in window.onload
                    }
                    return;
                }
                
                // Clear the flags since we have data now
                window.branchDataLoadInProgress = false;
                window.noBranchSelected = false;
                window.stopBranchChecks = false; // Ensure we don't stop checks once we have data
                window.noBranchMessageShown = false; // Reset the message flag
                
                // Update branch name
                const branchNameEl = document.getElementById('branchName');
                if (branchNameEl) {
                    // Preserve any SVG icon
                    const svgIcon = branchNameEl.querySelector('svg');
                    if (svgIcon) {
                        branchNameEl.innerHTML = '';
                        branchNameEl.appendChild(svgIcon);
                        branchNameEl.appendChild(document.createTextNode(' ' + (branchData.name || 'N/A')));
                    } else {
                        branchNameEl.textContent = branchData.name || 'N/A';
                    }
                }
                
                // Update branch address
                const branchAddressEl = document.getElementById('branchAddress');
                if (branchAddressEl) {
                    // Preserve any SVG icon
                    const svgIcon = branchAddressEl.querySelector('svg');
                    if (svgIcon) {
                        branchAddressEl.innerHTML = '';
                        branchAddressEl.appendChild(svgIcon);
                        branchAddressEl.appendChild(document.createTextNode(' ' + (branchData.address || 'N/A')));
                    } else {
                        branchAddressEl.textContent = branchData.address || 'N/A';
                    }
                }
                
                // Update stats
                if (typeof updateMaintenanceStats === 'function') {
                    updateMaintenanceStats();
                } else {
                    // Update counts directly if updateMaintenanceStats isn't available
                    const totalRooms = branchData.rooms ? Object.keys(branchData.rooms).length : 0;
                    const totalRoomsEl = document.getElementById('totalRooms');
                    if (totalRoomsEl) {
                        totalRoomsEl.textContent = totalRooms || '0';
                    }
                }
                
                // Update branch selector dropdown
                const branchSelect = document.getElementById('branchSelect');
                if (branchSelect && window.hotelBranchManager) {
                    const currentBranch = window.hotelBranchManager.getCurrentBranch();
                    if (currentBranch && branchSelect.value !== currentBranch) {
                        branchSelect.value = currentBranch;
                    }
                }
                
                console.log('Branch information updated in UI:', branchData.name);
            } catch (error) {
                console.error('Error updating branch info in UI:', error);
                // Don't retry - we'll rely on the window.onload check
            }
        }
        
        // Initialize dashboard stats
        function initializeDashboardStats() {
            // Set initial values to zero
            const newIssuesCount = document.getElementById('newIssuesCount');
            const totalRooms = document.getElementById('totalRooms');
            
            if (newIssuesCount) newIssuesCount.textContent = '0';
            if (totalRooms) totalRooms.textContent = '0';
            
            // The proper values will be set by updateMaintenanceStats() in form.js
        }
        
        // Optimized counter animation function using requestAnimationFrame
        function animateCounterOptimized(element, target, duration = 1000) {
            if (!element) return;
            
            const startTime = performance.now();
            const startValue = 0;
            
            function updateCounter(currentTime) {
                const elapsedTime = currentTime - startTime;
                const progress = Math.min(elapsedTime / duration, 1);
                
                const currentValue = Math.floor(progress * target);
                element.textContent = currentValue;
                
                if (progress < 1) {
                    requestAnimationFrame(updateCounter);
                } else {
                    element.textContent = target;
                }
            }
            
            requestAnimationFrame(updateCounter);
        }
        
        // Also call updateBranchInfoUI when the window is fully loaded
        window.onload = function() {
            console.log('Window loaded - checking branch data');
            
            // Reset any pending update flags to ensure we start fresh
            window.hasPendingBranchUpdate = false;
            window.branchDataLoadInProgress = false;
            window.noBranchSelected = false;
            window.branchErrorRetry = false;
            window.noBranchMessageShown = false;
            window.branchUpdateCallCount = 0;
            
            // First attempt immediately
            updateBranchInfoUI();
            
            // Schedule ONE additional check after 3 seconds in case initialization happens late
            setTimeout(() => {
                updateBranchInfoUI();
                
                // After this check, if we still don't have a branch selected, stop checking
                // to prevent console spam
                if (!window.hotelBranchManager || !window.hotelBranchManager.getCurrentBranch()) {
                    console.log('No branch selected after initial checks. Updates paused until branch selection.');
                    window.stopBranchChecks = true;
                    
                    // Add a listener to the branch selector dropdown to restart checks when a selection is made
                    const branchSelect = document.getElementById('branchSelect');
                    if (branchSelect) {
                        branchSelect.addEventListener('change', function() {
                            if (this.value) {
                                window.stopBranchChecks = false;
                                updateBranchInfoUI();
                            }
                        });
                    }
                }
            }, 3000);
        };
    </script>
    
